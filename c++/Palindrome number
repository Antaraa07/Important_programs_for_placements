//wap to check whether the number is palindrome or not
#include<iostream>
using namespace std;
int main()
{
int num,rev=0;
cout<<"Enter the number "; 
cin>>num; // taking the input 
int temp=n;
while(num>0)
{
rev=rev*10+num%10;
num/=10;
}
if (temp==rev)
{
cout<<"Number is palindrome ";
}
else
{
cout<<"Number is not palindrome";
}
return 0;
}
